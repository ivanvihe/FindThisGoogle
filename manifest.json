{
  "manifest_version": 3,
  "name": "FindThisGoogle",
  "version": "1.0",
  "description": "Search selected text in Google with exact match (quoted search)",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}